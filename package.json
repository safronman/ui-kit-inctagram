{
  "name": "@safronman/ui-kit-inctagram",
  "private": false,
  "version": "0.1.1",
  "description": "Reusable React UI kit components with typed variants and prebuilt styles.",
  "license": "MIT",
  "type": "module",
  "author": "",
  "homepage": "https://github.com/safronman/ui-kit-inctagram#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/safronman/ui-kit-inctagram.git"
  },
  "bugs": {
    "url": "https://github.com/safronman/ui-kit-inctagram/issues"
  },
  "keywords": [
    "react",
    "ui-kit",
    "components",
    "typescript",
    "design-system"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:lib && pnpm run build:types",
    "build:lib": "vite build",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "pnpm run build && pnpm run lint"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.575.0",
    "radix-ui": "^1.4.3",
    "tailwind-merge": "^3.5.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^10.2.12",
    "@storybook/addon-docs": "^10.2.12",
    "@storybook/addon-mcp": "^0.3.2",
    "@storybook/addon-onboarding": "^10.2.12",
    "@storybook/addon-vitest": "^10.2.12",
    "@storybook/react-vite": "^10.2.12",
    "@storybook/types": "8.6.14",
    "@tailwindcss/vite": "^4.2.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "4.0.18",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.2.12",
    "globals": "^16.5.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "shadcn": "^3.8.5",
    "storybook": "^10.2.12",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "^4.2.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "4.0.18"
  },
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx",
      "./*.js",
      "./*.jsx"
    ]
  }
}
